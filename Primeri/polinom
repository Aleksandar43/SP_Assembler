.text
main:
	@r0 - x
	@r1 - pok. na 1. element niza polinoma
	@r2 - stepen polinoma
	mov r3,r0
	mov r4,r1
	ldr r0,r4,2,0 @r0=r4, r4=a[1]
	cmp r2,-1 f
	mov pc,exit ge
	ldr r1,r4,2,0 @r1=[r4+0], r4=r4+4
	mul r0,r3 @a*x
	add r0,r1 @a*x+b
	sub r2,1
exit: mov pc,lr @r0=rezultat
	call printf
.end
	//5x^3+2x^2-4x+9=(5x^2+2x-4)*x+9=((5x+2)*x-4)*x+9=(((5)*x+2)*x-4)*x+9